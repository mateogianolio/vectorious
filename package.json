{
  "name": "vectorious",
  "version": "6.0.0",
  "description": "A high performance linear algebra library.",
  "main": "built/index.js",
  "scripts": {
    "test:node": "nyc mocha -r ts-node/register/transpile-only src/**/*.spec.ts",
    "test:single": "mocha -r ts-node/register/transpile-only",
    "test:browser": "mochify --plugin tsify src/**/*.spec.ts",
    "test": "npm run test:node && npm run test:browser",
    "lint": "tslint -p . -c tslint.json 'src/*.ts'",
    "benchmark": "for file in $(ls src/**/*.bench.ts); do ts-node --transpile-only $file; done",
    "docs": "gulp docs",
    "build": "gulp clean && gulp build && gulp docs && tsc"
  },
  "browser": {
    "nblas": false,
    "nlapack": false
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "gulpfile.js",
      "**/*.d.ts",
      "benchmarks/**",
      "built/**",
      "dist/**",
      "docs/**",
      "examples/**",
      "coverage/**",
      "src/**/*.spec.ts",
      "src/**/*.bench.ts",
      "src/util.ts",
      "src/bench.ts"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mateogianolio/vectorious.git"
  },
  "keywords": [
    "matrix",
    "vector",
    "linear",
    "algebra"
  ],
  "author": "Mateo Gianolio",
  "contributors": [
    "Bart van Andel <bavanandel@gmail.com>",
    "James Vickers",
    "Phillip Wang",
    "Waylon Flinn",
    "Xuefeng Zhu"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mateogianolio/vectorious/issues"
  },
  "homepage": "https://github.com/mateogianolio/vectorious",
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.9",
    "benchmark": "^2.1.4",
    "browserify": "^16.3.0",
    "del": "5.0.0",
    "gulp": "^4.0.2",
    "gulp-streamify": "^1.0.2",
    "gulp-typedoc": "^2.2.2",
    "gulp-uglify": "^3.0.2",
    "matplotnode": "^0.4.2",
    "mocha": "^6.2.0",
    "mochify": "^6.3.0",
    "nodemark": "^0.3.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tsify": "^4.0.1",
    "tslint": "^5.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3",
    "vinyl-source-stream": "^2.0.0"
  },
  "optionalDependencies": {
    "nblas": "^2.1.5",
    "nlapack": "^2.0.0"
  },
  "dependencies": {}
}
