  },
  "keywords": [
    "matrix",
    "vector",
    "linear",
    "algebra"
  ],
  "author": "Mateo Gianolio",
  "contributors": [
    "Bart van Andel <bavanandel@gmail.com>",
    "James Vickers",
    "Phillip Wang",
    "Waylon Flinn",
    "Xuefeng Zhu"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mateogianolio/vectorious/issues"
  },
  "homepage": "https://github.com/mateogianolio/vectorious",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.1",
    "benchmark": "^2.1.4",
    "better-docs": "^2.3.2",
    "browserify": "^16.5.2",
    "del": "5.1.0",
    "gulp": "^4.0.2",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^3.0.2",
    "jest": "^26.4.2",
    "jsdoc": "^3.6.5",
    "matplotnode": "^0.4.2",
    "mocha": "^8.1.3",
    "mochify": "^6.6.0",
    "nodemark": "^0.3.0",
    "tidy-jsdoc": "^1.4.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsify": "^5.0.2",
    "typescript": "^4.0.2",
    "vinyl-source-stream": "^2.0.0"
  },
  "optionalDependencies": {
    "nblas": "^2.1.9"
  },
  "dependencies": {
    "nlapack": "^2.0.4"
